			regulator-max-microvolt = <2200000>;
			regulator-enable-ramp-delay = <120>;
		};
		mt_pmic_vefuse_ldo_reg: ldo_vefuse {
			regulator-name = "vefuse";
			regulator-min-microvolt = <1700000>;
