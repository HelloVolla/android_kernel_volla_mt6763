&ufshci {
	pinctrl-names =
			"state_va09_on",
			"state_va09_off";

	pinctrl-0 = <&ufshci_pins_state_va09_on>;
	pinctrl-1 = <&ufshci_pins_state_va09_off>;
	status = "okay";
};

&pio {
	ufshci_pins_state_va09_on: ufshci_va09on {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO131__FUNC_GPIO131>;
			slew-rate = <1>;  /* direction: output */
			output-low;      /* output: VA09 ON */
			bias-disable;     /* pull: disabled */
		};
	};
	ufshci_pins_state_va09_off: ufshci_va09off {
		pins_cmd_dat {
			pinmux = <PINMUX_GPIO131__FUNC_GPIO131>;
			slew-rate = <1>;  /* direction: output */
			output-high;       /* output: VA09 OFF */
			bias-disable;     /* pull: disabled */
		};
	};
};

